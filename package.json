    "concurrently": "^4.0.1",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-beta.3",
    "dotenv": "^6.0.0",
    "es6-promise": "^4.2.5",
    "express": "^4.16.3",
    "express-messages": "^1.0.1",
    "express-session": "^1.15.6",
    "express-validator": "^5.3.0",
    "image-downloader": "^3.4.1",
    "isomorphic-fetch": "^2.2.1",
    "mongoose": "^5.2.18",
    "node-fetch": "^2.2.0",
    "nodemon": "^1.18.4",
    "parse-mongo-url": "^1.1.1",
    "passport": "^0.4.0",
