{
  "name": "igallery",
  "version": "1.0.0",
  "description": "A simple photo sharing web app",
  "main": "server.js",
  "scripts": {
    "server": "nodemon server.js",
    "client": "npm run start --prefix clientside",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "start": "node server.js",
    "heroku-postbuild": "cd clientside && npm install --only=dev && npm install && npm run build"
